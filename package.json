{
    "name": "php-double-dollar-detection-vscode",
    "displayName": "PHP Double Dollar Detection",
    "version": "0.0.0",
    "description": "PHP Double Dollar Detection VSCode Extension",
    "publisher": "x-shuffle",
    "main": "extension.js",
    "scripts": {
        "start": "node extension.js",
        "dev": "node extension.js"
    },
    "author": "X-shuffle",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/X-shuffle/php-double-dollar-detection-vscode.git"
    },
    "engines": {
        "vscode": "^1.102.2"
    },
    "activationEvents": [
        "onLanguage:php",
        "onCommand:php-double-dollar.hello",
        "onCommand:php-double-dollar.check"
    ],
    "contributes": {
        "commands": [
            {
                "command": "php-double-dollar.hello",
                "title": "PHP: Hello"
            },
            {
                "command": "php-double-dollar.check",
                "title": "PHP: 检查$$符号"
            }
        ]
    }
}